{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "module": "es2022",
    "moduleResolution": "node",
    "target": "es2022",
    "sourceMap": false,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "es2022"
    ]
  },
  "include": [
    "**/*.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
